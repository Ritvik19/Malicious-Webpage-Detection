{
  "manifest_version" : 2,
  "name" : "Webpage Filter",
  "description" : "Browse the internet safer than ever before",
  "version" : "0.1",
  "web_accessible_resources": ["src/classifier.js"],
  "content_scripts" : [
    {
      "matches": ["<all_urls>"],
      "js": ["src/loader.js"]
    }
  ]
}
